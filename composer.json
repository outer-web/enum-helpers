{
    "name": "outerweb/enum-helpers",
    "description": "Laravel traits to enhance php enums",
    "homepage": "https://github.com/outer-web/enum-helpers",
    "license": "MIT",
    "authors": [
        {
            "name": "Outerweb",
            "email": "info@outerweb.be"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0|^11.0",
        "spatie/laravel-package-tools": "^1.16"
    },
    "autoload": {
        "psr-4": {
            "Outerweb\\EnumHelpers\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Outerweb\\EnumHelpers\\EnumHelpersServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
